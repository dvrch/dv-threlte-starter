{
	"version": 2,
	"builds": [
		{
			"src": "api/index.py",
			"use": "@vercel/python"
		},
		{
			"src": "package.json",
			"use": "@vercel/svelte-kit"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "api/index.py"
		},
		{
			"src": "/admin/(.*)",
			"dest": "api/index.py"
		},
		{
			"src": "/health/(.*)",
			"dest": "api/index.py"
		},
		{
			"src": "/(.*)",
			"dest": "/$1"
		}
	],
	"env": {
		"PYTHONPATH": "/var/task",
		"DJANGO_SETTINGS_MODULE": "backend.settings",
		"SECRET_KEY": "votre-secret-key",
		"DEBUG": "False",
		"ALLOWED_HOSTS": "dv-threlte-starter.vercel.app,.vercel.app"
	}
}
