{
  "builds": [
    {
      "src": "backend/films_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "package.json",
      "use": "@vercel/sveltekit"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/films_backend/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
